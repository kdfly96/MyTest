<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Group Suite">
	<test thread-count="5" name="Booking Test">
		<groups>
			<run>
				<include name="BOOK"></include>
			</run>
		</groups>
		<classes>
			<class name="groupintest.GroupTest" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="Cancel Test">
		<groups>
			<run>
				<include name="CANCEL"></include>
			</run>
		</groups>
		<classes>
			<class name="groupintest.GroupTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
